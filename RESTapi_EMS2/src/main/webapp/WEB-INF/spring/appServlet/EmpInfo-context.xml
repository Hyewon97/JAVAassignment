<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- EmpInfoDaoImp 빈 선언 -->
	<bean id="empInfoDao" class="EmpInfo.dao.EmpInfoImp">
		<property name="sqlSession" ref="sqlSession" />
	</bean>

	<!-- EmpInfoServiceImp 빈 선언 -->
	<bean id="empInfoService" class="EmpInfo.service.EmpInfoServiceImp">
		<property name="empInfoDao" ref="empInfoDao" />
	</bean>

	<!-- BoardController 빈 선언 
	<bean class="board.controller.BoardController">
		<property name="boardService" ref="boardService" />
	</bean>
	-->
</beans>
